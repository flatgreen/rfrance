{
    "name": "flatgreen/rfrance",
    "description": "Scrape R A D I O  F R A N C E",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Flatgreen\\RFrance\\": "src/"
        },
        "files": [
            "src/functions.php"
        ],
            "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "require": {
        "php": ">=8.0.2",
        "symfony/cache": "^6.0|^7.0",
        "symfony/dom-crawler": "^6.0|^7.0",
        "symfony/css-selector": "^6.0|^7.0",
        "colinodell/json5": "^3.0"
    },
    "scripts": {
        "phpstan": "phpstan analyse --level 8 src/",
        "tests": "phpunit tests/"
    },
    "license": "MIT",
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^10.5",
        "symfony/var-dumper": "^6.4"
    }
}
